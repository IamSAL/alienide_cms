{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "sub_title": {
      "type": "string",
      "required": false
    },
    "header_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "prebuilt",
        "custom",
        "leaf"
      ],
      "required": true,
      "default": "leaf"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "media.gallery",
        "text.keyword"
      ],
      "required": false
    },
    "leaf_content": {
      "type": "richtext"
    },
    "status": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "mixed.seo"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "header_size": {
      "type": "enumeration",
      "enum": [
        "full",
        "half"
      ],
      "default": "full",
      "required": true
    }
  }
}
