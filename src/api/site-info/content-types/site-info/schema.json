{
  "kind": "singleType",
  "collectionName": "site_infos",
  "info": {
    "singularName": "site-info",
    "pluralName": "site-infos",
    "displayName": "Site info",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "site_name": {
      "type": "string",
      "required": true,
      "default": "My website"
    },
    "site_title": {
      "type": "string"
    },
    "meta_keywords": {
      "type": "component",
      "repeatable": true,
      "component": "text.keyword"
    },
    "site_logo": {
      "type": "component",
      "repeatable": false,
      "component": "media.logos",
      "required": true
    },
    "contact_info": {
      "type": "component",
      "repeatable": false,
      "component": "elements.contact-info",
      "required": true
    },
    "main_menu": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::menu.menu"
    },
    "footer_menu_left": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::menu.menu"
    },
    "footer_menu_right": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::menu.menu"
    }
  }
}
